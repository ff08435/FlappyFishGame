using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class NewBehaviourScript : MonoBehaviour
{
    private Rigidbody2D _rb;
    [SerializeField] private float _upForce = 10; //moving force


    private void Awake()
    {
        _rb = GetComponent<Rigidbody2D>(); //get the ref for rigibody component
       
    }

    void Update()
    {
        if (Input.GetKeyDown(KeyCode.Space))

        {
            
            _rb.velocity = Vector2.up * _upForce; //make the fish move up n down
        }


    }

    private void OnCollisionEnter2D(Collision2D collision) //game over
    {
        GameManager.instance.GameOver();

    }
}
